{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/zh/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next|static|favicon.ico|manifest.json|robots.txt).*)",
      "destination": "/index.html"
    }
  ]
}
